<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/title_layout" />


    <include
        android:id="@+id/showLoadError"
        layout="@layout/showingpage_load_error"
        android:visibility="gone" />

    <include
        android:id="@+id/showLoading"
        layout="@layout/showingpage_loading"
        android:visibility="gone" />


    <include
        android:id="@+id/unloaditem"
        layout="@layout/showingpage_unload"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/showSuccessView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />


</LinearLayout>